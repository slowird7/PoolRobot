                  <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onBtnAimClicked" prefHeight="25.0" prefWidth="42.0" text="Aim">
