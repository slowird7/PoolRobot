               </HBox.margin>
            </ChoiceBox>
            <ChoiceBox fx:id="cbPocket" layoutX="224.0" layoutY="14.0" prefHeight="25.0" prefWidth="56.0">
               <HBox.margin>
                  <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
               </HBox.margin>
            </ChoiceBox>
            <ComboBox fx:id="selGame" onAction="#onSelGameSelected" prefHeight="25.0" prefWidth="90.0">
