<AnchorPane id="AnchorPane" fx:id="mainWindow" onKeyPressed="#onKeyPressed" prefHeight="312.0" prefWidth="564.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="poolrobot.MainWindowController">
